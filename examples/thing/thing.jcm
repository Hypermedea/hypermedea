/*
 * Note: this example assumes a "smart coffee machine" is running locally.
 * This Thing is provided as example in the node-wot distribution. See
 * https://github.com/eclipse-thingweb/node-wot/tree/master/examples/scripts
 */
mas thing_mas {

   agent consumer {
          goals: start
          join: thing_workspace
          focus: thing_workspace.h
    }

    workspace thing_workspace  {
        artifact h: org.hypermedea.HypermedeaArtifact()
    }

}
